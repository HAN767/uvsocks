<?xml version="1.0" encoding="utf-8"?>
<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets" />
  <PropertyGroup Label="UserMacros">
    <HeedongLib>$(SolutionDir)lib\$(Platform)</HeedongLib>
    <HeedongLibInclude>$(HeedongLib)\include</HeedongLibInclude>
    <HeedongLibLib>$(HeedongLib)\lib</HeedongLibLib>
    <HeedongLibBin>$(HeedongLib)\bin</HeedongLibBin>
    <HeedongName>libuvsocks</HeedongName>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(ProjectDir)\src;$(HeedongLibInclude);$(HeedongLibInclude)\json-glib-1.0;$(HeedongLibInclude)\glib-2.0;$(HeedongLibLib)\glib-2.0\include;$(HeedongLibInclude)\libuv;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>_CRT_SECURE_NO_WARNINGS;_WINSOCK_DEPRECATED_NO_WARNINGS;_DEBUG;PACKAGE="$(HeedongName)";G_DISABLE_DEPRECATED</PreprocessorDefinitions>
      <DisableSpecificWarnings>4819</DisableSpecificWarnings>
    </ClCompile>
    <Link>
      <AdditionalDependencies>%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalLibraryDirectories>$(HeedongLibLib);$(HeedongLibBin)</AdditionalLibraryDirectories>
      <ImageHasSafeExceptionHandlers>false</ImageHasSafeExceptionHandlers>
    </Link>
    <PreBuildEvent>
    </PreBuildEvent>
  </ItemDefinitionGroup>
  <ItemGroup>
    <BuildMacro Include="HeedongLib">
      <Value>$(HeedongLib)</Value>
    </BuildMacro>
    <BuildMacro Include="HeedongLibInclude">
      <Value>$(HeedongLibInclude)</Value>
    </BuildMacro>
    <BuildMacro Include="HeedongLibLib">
      <Value>$(HeedongLibLib)</Value>
    </BuildMacro>
    <BuildMacro Include="HeedongLibBin">
      <Value>$(HeedongLibBin)</Value>
    </BuildMacro>
    <BuildMacro Include="HeedongName">
      <Value>$(HeedongName)</Value>
    </BuildMacro>
  </ItemGroup>
</Project>